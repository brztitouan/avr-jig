 113               	#   define ptrH     ZH
 114               	#   define ptr      Z
 115               	#   define byte     r22
 116               	#   define bitCnt   r19
 117               	#   define polyL    r20
 118               	#   define polyH    r21
 119               	#   define scratch  r23
