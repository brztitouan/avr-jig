  35:usbdrv/usbdrvasm16.inc ****     push    YL                  ;[-25] push only what is necessary to sync with edge ASAP
  36:usbdrv/usbdrvasm16.inc ****     in      YL, SREG            ;[-23]
  37:usbdrv/usbdrvasm16.inc ****     push    YL                  ;[-22]
  38:usbdrv/usbdrvasm16.inc ****     push    YH                  ;[-20]
